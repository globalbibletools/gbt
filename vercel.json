{
  "rewrites": [
    {
      "source": "/([^.]+)",
      "has": [
        {
          "type": "host",
          "value": "interlinear.globalbibletools.com"
        }
      ],
      "destination": "/"
    },
    {
      "source": "/([^.]+)",
      "has": [
        {
          "type": "host",
          "value": "gloss-translation(?<host>-(?!api).*|).vercel.app"
        }
      ],
      "destination": "/"
    }
  ]
}
